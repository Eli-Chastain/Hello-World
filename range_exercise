for i in range(10, 16):
    print("i is now {}".format(i))
for i in range(0, 10):
    print(i)

for i in range(10, -4, -2):
    print("i is now {}".format(i))
for i in range(0, 101, 7):
    print(i)

for i in range(1, 13):
    for j in range(1, 13):
        print("{0} times {1} is {2}".format(j, i, i * j))


